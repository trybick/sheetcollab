generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id         Int     @default(autoincrement()) @id
  updatedAt  DateTime @default(now())
  createdAt  DateTime @default(now())
  email      String  @unique
  password   String
  name       String?
  sheets     Sheet[]
}

model Sheet {
  id        Int     @default(autoincrement()) @id
  updatedAt DateTime @default(now())
  createdAt DateTime @default(now())
  title     String
  artist    String
  year      String?
}