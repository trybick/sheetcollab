generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id          Int        @default(autoincrement()) @id
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @default(now())
  email       String     @unique
  password    String
  name        String?
  sheets      Sheet[]
}

model Sheet {
  id          Int        @default(autoincrement()) @id
  createdAt   DateTime   @default(now())
  updatedAt   DateTime   @default(now())
  artist      String
  title       String
  users       User[]
  year        String?
}