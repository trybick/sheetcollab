generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  email      String  @unique
  password   String
  id         Int     @default(autoincrement()) @id
  name       String?
  sheets     Sheet[]
}

model Sheet {
  id        Int     @default(autoincrement()) @id
  title     String
  artist    String
  year      String?
}